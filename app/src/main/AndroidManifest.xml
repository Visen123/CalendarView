<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yanyiyun.calendarviewproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.yanyiyun.calendarviewproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.yanyiyun.calendarviewproject.meizu.MeiZuActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.index.IndexActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.simple.SimpleActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.range.RangeActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.colorful.ColorfulActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.solay.SolarActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.single.SingleActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.progress.ProgressActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.custom.CustomActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.pager.ViewPagerActivity" />
        <activity android:name="com.yanyiyun.calendarviewproject.TestActivity" />
    </application>

</manifest>